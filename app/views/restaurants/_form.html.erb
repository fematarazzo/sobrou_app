<%= simple_form_for restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category, collection: ["Árabe", "Chinesa", "Japonesa", "Lanche", "Marmita", "Padaria", "Pizza"]  %>
  <%= f.input :phone %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class:"btn-dashboard btn-primeiro" %>
<% end %>

